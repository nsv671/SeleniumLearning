public class LandingPage {
	WebDriver driver;
	
	public LandingPage(WebDriver driver) {
		this.driver = driver;
	}

	driver.f
	driver.findElement(By.id("userEmail")).sendKeys("test671@test.com");
	
	@FindBy(id="userEmail")
	WebElement email;
}
